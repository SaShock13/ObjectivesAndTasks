<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ObjectivesAndTasks.AddObjectivePage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Страница добавления"/>
            <Entry x:Name="entName" Placeholder="Введите Цель" Text="Задача1"></Entry>
            <Entry x:Name="entDescription" Placeholder="Введите описание цели" Text="OBJECTIVE1 execution"></Entry>
            <Entry x:Name="entPriority" Placeholder="Введите приоритет от 1 до 3" Text="1"></Entry>
            <Button x:Name="btnAcceptAddObjective" Text="Принять" Clicked="btnAcceptAddObjectiveClicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>